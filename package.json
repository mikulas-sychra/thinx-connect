{
  "name": "THiNX-Connect",
  "version": "1.0.10",
  "description": "http-to-https proxy for thinx.cloud",
  "main": "app.js",
  "start": "nohup forever --minUptime 1000 --spinSleepTime 5000 /thinx-connect/app.js",
  "scripts": {
    "test": "snyk test && nyc jasmine && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/suculent/thinx-connect.git"
  },
  "author": "Matej Sychra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suculent/thinx-connect/issues"
  },
  "homepage": "https://github.com/suculent/thinx-connect#readme",
  "dependencies": {
    "colors": "^1.1.2",
    "colours": "^0.6.0-2",
    "http-proxy": "^1.16.2",
    "mdns": "^2.3.3",
    "rollbar": "^2.0.2",
    "snyk": "^1.31.1",
    "ssl-root-cas": "^1.2.3",
    "sync-exec": "^0.6.2"
  },
  "devDependencies": {
    "codecov": "^2.2.0",
    "coveralls": "^2.13.1",
    "grunt": "^1.0.1",
    "grunt-karma-coveralls": "^2.5.4",
    "jasmine-core": "^2.6.2",
    "karma": "^1.7.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.3.2"
  },
  "snyk": true
